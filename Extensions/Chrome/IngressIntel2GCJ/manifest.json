{
  "manifest_version": 2,
  "name": "IngressIntel2GCJ",
  "version": "0.2.1",
  "description": "fix the Ingress Intel map to GCJ02",
  "page_action": {
    "default_icon": "icons/Icon48.png",
    "default_title": "IngressIntel2GCJ",
    "default_popup": "popup.html"
  },
  "permissions": [
    "http://*/",
    "https://*/",
    "bookmarks",
    "tabs",
    "history"
  ] ,
  "icons": { "16": "icons/Icon16.png",
    "48": "icons/Icon48.png",
    "128": "icons/Icon128.png" },
  "content_scripts": [{
      "matches": ["https://www.ingress.com/intel*"],
      "css": ["style.css"],
      "js": ["/js/jquery-2.1.4.js","/js/Init.js"]
    }]
}