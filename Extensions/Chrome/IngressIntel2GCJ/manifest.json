{
  "manifest_version": 2,
  "name": "IngressIntel2GCJ",
  "version": "0.2.1",
  "description": "fix the Ingress Intel map to GCJ02",
  "browser_action": {
    "default_icon": "Icon128.png"
  },
  "permissions": [
    "http://*/",
    "https://*/",
    "bookmarks",
    "tabs",
    "history"
  ] ,
  "icons": { "16": "Icon16.png",
    "48": "Icon48.png",
    "128": "Icon128.png" },
  "content_scripts": [
    {
      "matches": ["https://www.ingress.com/*"],
      "css": ["style.css"],
      "js": ["jquery-2.1.4.js","Init.js"]
    }
  ]
}